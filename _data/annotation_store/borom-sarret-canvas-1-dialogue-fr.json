      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "- Salamalécoum, frère ! Tu me conduis au cimetière ?",
        "format": "text/plain",
        "purpose": "commenting"
